<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
	<div class="header-container">
		<header class="width">

		</header>
	</div>
	<div class="nav-container">
		<nav class="width">
			<ul>
				<li><a href="/">Home</a></li>
				<li><a href="NewUser">Users</a></li>
				<li><a href="Writings">Submit Writings</a></li>
				<li><a href="listwritings">View Writings</a></li>
				<li><a href="newcontest">Join Contest</a></li>
				<li><a href="contests">View Contests</a></li>
			</ul>
		</nav>
	</div>
	<div class="main-container">
		<div class="main width">
			<h1>View Contests</h1>
			<div class="contestList"></div>
			<script>
				$.get( "getcontests", function( data ) {
					var obj = $.parseJSON(data);
					for(var i in obj)
					{
						var title = obj[i].title;
						var publisher_id = obj[i].publisher_id;
						var dateStart = obj[i].dateStart;
						var dateEnd = obj[i].dateEnd;
						var genre  = obj[i].Genre;
						
						$('.contestList').append('<div><div>'+title+'</div>');
						$('.contestList').append('<div>'+publisher_id+'</div>');
						$('.contestList').append('<div>'+dateStart+'</div>');
						$('.contestList').append('<div>'+dateEnd+'</div>');
						$('.contestList').append('<div>'+genre+'</div></div><hr>');
					}
				});
			</script>
		</div>
	</div>
	<div class="footer-container">
		<footer class="width">
			&copy; 2015 Evan & Alex.  All Rights Reserved.
		</footer>
	</div>
  </body>
</html>
